                            <TextBlock  Grid.Column="7" Text=" New MAC:" Margin="0,0,10,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="txtNewDeviceMAC"  Grid.Column="8" Padding="2" Margin="0 0 10 0"/>
                            <Button x:Name="btnUpdateFactory" Content="Update Factory" Grid.Column="9" Click="btnUpdateFactory_Click"  Margin="0,0,10,0"/>

                        </Grid>

                        <Grid Margin="0 0 0 30">

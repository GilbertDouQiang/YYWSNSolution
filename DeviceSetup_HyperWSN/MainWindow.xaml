                            <TextBox  Text="{Binding TemperatureWarnHigh}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" />
                            <TextBox x:Name="txtTemperatureWarnHigh"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />



                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>

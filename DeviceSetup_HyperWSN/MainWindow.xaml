                            <TextBlock   Grid.Column="3" Text="采/发倍数:" Margin="0,0,9.5,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="txtTXTimers" Text="{Binding TXTimers}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" />
                            <TextBox x:Name="txtNewTXTimers"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />
                            <TextBlock  Grid.Column="6" Text=" 内部时钟:" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="txtCalendar"   Grid.Column="7" Padding="2" Margin="0.5,0,10,0"   Grid.ColumnSpan="2">
                                <TextBox.Text>
                                    <Binding Path="Calendar" StringFormat="{}{0:yyyy-MM-dd HH:mm:ss}"/>
                                </TextBox.Text>
                            </TextBox>
                            <ComboBox x:Name="cbUserApplication" Grid.Column="10" Grid.ColumnSpan="2" SelectionChanged="cbUserApplication_SelectionChanged" />


                        </Grid>

                        <Grid Margin="0 0 0 10">

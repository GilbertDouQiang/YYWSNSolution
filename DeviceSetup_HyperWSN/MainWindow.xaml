<Window x:Class="DeviceSetup_HyperWSN.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeviceSetup_HyperWSN"
        mc:Ignorable="d"
        Title="HyperWSN Device Setup" Height="680" Width="1100" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" Icon="timg_256.ico">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="1.4*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        <Border Padding="5" Grid.Row="0">
            <StackPanel>
                <Grid Margin="0 10 0 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.4*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="6*"/>
                        <ColumnDefinition Width="1.5*"/>
                        <ColumnDefinition Width="1.5*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="0.4*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Text="端口号:" Grid.Column="1" FontWeight="Bold" Margin="0,3,9,2" VerticalAlignment="Center" HorizontalAlignment="Right" />
                    <ComboBox x:Name="cbSerialPort" Grid.Column="2" Margin="0 0 10 0" VerticalAlignment="Center" Grid.ColumnSpan="1" SelectionChanged="cbSerialPort_SelectionChanged"/>
                    <Button x:Name="btnOpenComport" Grid.Column="3" Content="Open" Click="btnOpenComport_Click"  Margin="0,1,10,1" VerticalAlignment="Center"/>
                    <Button x:Name="btnFindComport" Grid.Column="4" Content="Refersh" Click="btnFindComport_Click" Margin="0.5,0,11,1" VerticalAlignment="Bottom"/>
                </Grid>
                <Grid Margin="0 10 0 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="0.7*"/>
                        <ColumnDefinition Width="0.7*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="0.5*"/>

                    </Grid.ColumnDefinitions>

                    <Button x:Name="btnStartMonitor" Grid.Column="2" Click="btnStartMonitor_Click" Content="Start Monitor" Margin="0,0,0,0" VerticalAlignment="Bottom"/>
                    <TextBox x:Name="tbxRssiThr"   Grid.Column="3" Padding="2" Margin="10,0,3,0"   />
                    <TextBlock  Grid.Column="4" Text="dBm" Margin="0,3,0,0" HorizontalAlignment="Left"/>
                    <Button x:Name="btnStopMonitor" Grid.Column="5" Content="Stop Monitor"  Click="btnStopMonitor_Click"   Margin="5,0" VerticalAlignment="Center"/>
                    <Button x:Name="btnCopy" Grid.Column="7" Content="Copy"  Click="btnCopy_Click"   Margin="5,0" VerticalAlignment="Center"/>
                    <Button x:Name="btnClear" Grid.Column="8" Content="Clear"  Click="btnClear_Click"   Margin="5,0" VerticalAlignment="Center"/>

                </Grid>
            </StackPanel>
        </Border>
        <Border Padding="5" Grid.Row="1">
            <TabControl Margin="5,0,5,5" >

                <TabItem x:Name="TableOfM1" Header=" M1 | M2 ">
                    <StackPanel x:Name="StackOfM1">
                        
                        <Grid Margin="0 20 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>

                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" Text="设备名称:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox Grid.Column="1" x:Name="tbxDeviceNameOfM1"  Text="{Binding Name}"  Padding="2" Margin="0 0 10 0"/>
                            
                            <TextBlock Grid.Column="2" Text="设备类型:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox Grid.Column="3" x:Name="tbxDeviceTypeOfM1"  Text="{Binding DeviceTypeV,StringFormat='{}{0:X2}'}"  Padding="2" Margin="0 0 10 0"/>
                            
                            <TextBlock Grid.Column="4" Text=" 协议版本号:"  Margin="0,0,10,0" HorizontalAlignment="Right"/>
                            <TextBox Grid.Column="5" x:Name="tbxProtocolOfM1" Text="{Binding ProtocolVersion}"  Padding="2" Margin="0,0,10,0"/>

                            <Button  Grid.Column="8" x:Name="btnUpdateFactoryOfM1" Content="Update Factory" Click="btnUpdateFactoryOfM1_Click"  Margin="0,0,10,0"/>
                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="1.4*"/>
                                <ColumnDefinition Width="0.6*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                            </Grid.ColumnDefinitions>
                            
                            <TextBlock Grid.Column="0" Text="PrimaryMac:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox Grid.Column="1" x:Name="tbxPrimaryMacOfM1" Text="{Binding PrimaryMacS}"   Margin="0 0 10 0" />
                            
                            <TextBlock Grid.Column="2" Text=" DeviceMac:" Margin="0,0,10,0" HorizontalAlignment="Right"/>
                            <TextBox Grid.Column="3" x:Name="tbxDeviceMacOfM1"  Text="{Binding DeviceMacS}"  Padding="2" Margin="0 0 0 0"/>

                            <CheckBox Grid.Column="4" x:Name="cbxIsAllOfM1"  Content="All" HorizontalAlignment="Left" Margin="2,0,0,0" VerticalAlignment="Center"/>

                            <TextBlock  Grid.Column="5" Text=" New Mac:" Margin="0,0,10,0" HorizontalAlignment="Right"/>
                            <TextBox  Grid.Column="6" x:Name="tbxNewDeviceMacOfM1"  Padding="2" Margin="0 0 10 0"/>

                           
                        </Grid>

                        <Grid Margin="0 0 0 30">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Column="0" Text=" 软件版本:"  Margin="0,0,10,0" HorizontalAlignment="Right"/>
                            <TextBox Grid.Column="1" x:Name="tbxSwRevisionOfM1" Text="{Binding SwRevisionS}" Padding="2" Margin="0,0,10,0"/>

                            <TextBlock Grid.Column="2" Text="硬件版本:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox Grid.Column="3" x:Name="tbxHwRevisionOfM1" Text="{Binding HwRevisionS}" Padding="2" Margin="0,0,10,0"/>
                            
                            <TextBlock Grid.Column="4" Text="新硬件版本:" Margin="0,0,10,0" HorizontalAlignment="Right"/>
                            <TextBox Grid.Column="5" x:Name="tbxNewHwRevisionOfM1"   Padding="2" Margin="0,0,10,0"/>
                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>
                            <TextBlock  Grid.Column="0" Text=" 客户码:" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxCustomerOfM1" Text="{Binding CustomerS}" Grid.Column="1" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBox x:Name="tbxNewCustomerOfM1"   Grid.Column="2" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBlock   Grid.Column="3" Text="Debug状态:" Margin="0,0,9.5,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxDebugOfM1" Text="{Binding DebugS}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" />
                            <TextBox x:Name="tbxNewDebugOfM1"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />
                            <TextBlock  Grid.Column="6" Text=" 分类码:" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxCategoryOfM1" Text="{Binding Category}"  Grid.Column="7" Padding="2" Margin="0,0,10,0"   />
                            <TextBox x:Name="tbxNewCategoryOfM1"   Grid.Column="8" Padding="2" Margin="0,0,10,0"   />
                            <Button x:Name="btnUpdateUserOfM1" Content="Update User Config" Grid.Column="10" Grid.ColumnSpan="2" Click="btnUpdateUserOfM1_Click"  Margin="0,0,10,0" Height="21" VerticalAlignment="Top"/>
                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="0" Text="工作模式:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxPatternOfM1" Text="{Binding Pattern}"  Grid.Column="1" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewPatternOfM1"   Grid.Column="2" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="3" Text="传输速率:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxBpsOfM1" Text="{Binding Bps}"  Grid.Column="4" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewBpsOfM1"   Grid.Column="5" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="6" Text="发射功率:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxTxPowerOfM1" Text="{Binding TxPower}"  Grid.Column="7" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewTxPowerOfM1"  Grid.Column="8" Padding="2" Margin="0 0 10 0"/>                           
                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="0" Text="频段:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxChannelOfM1" Text="{Binding Channel}" Grid.Column="1" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewChannelOfM1"  Grid.Column="2" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="3" Text="温度补偿:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxTemperatureCompensationOfM1" Text="{Binding TempCompensation}" Grid.Column="4" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewTemperatureCompensationOfM1"  Grid.Column="5" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="6" Text="湿度补偿:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxHumidityCompensationOfM1" Text="{Binding HumCompensation}" Grid.Column="7" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewHumidityCompensationOfM1"  Grid.Column="8" Padding="2" Margin="0,0,10,0"  Height="21" VerticalAlignment="Bottom"/>
                        </Grid>

                        <Grid Margin="0 0 0 20">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="0" Text="存储容量:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxMaxLengthOfM1" Text="{Binding MaxLength}" Grid.Column="1" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewMaxLengthOfM1"  Grid.Column="2" Padding="2" Margin="0 0 10 0"/>
                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            
                            <TextBlock   Grid.Column="3" Text="采/发倍数:" Margin="0,0,9.5,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxSampleSendOfM1" Text="{Binding SampleSend}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" />
                            <TextBox x:Name="tbxNewSampleSendOfM1"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />
                            <TextBlock  Grid.Column="0" Text=" 内部时钟:" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxCalendarOfM1"   Grid.Column="1" Padding="2" Margin="0.5,0,10,0"   Grid.ColumnSpan="2">
                                <TextBox.Text>
                                    <Binding Path="Calendar" StringFormat="{}{0:yyyy-MM-dd HH:mm:ss}"/>
                                </TextBox.Text>
                            </TextBox>
                            <TextBlock  Grid.Column="6" Text=" 采集间隔:" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxIntervalOfM1" Text="{Binding Interval}"  Grid.Column="7" Padding="2" Margin="0,0,5,0"/>
                            <TextBox x:Name="tbxNewIntervalOfM1"  Grid.Column="8" Padding="2" Margin="0,0,5,0"/>
                            <Button x:Name="btnUpdateApplicationOfM1" Content="Update App Config" Grid.Column="10" Grid.ColumnSpan="2" Click="btnUpdateApplicationOfM1_Click" Margin="0,0,10,0"/>
                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>
                            <TextBlock  Grid.Column="0" Text=" 预警温度(低):" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxTemperatureWarnLowOfM1" Text="{Binding TempWarnLow}"  Grid.Column="1" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBox x:Name="tbxNewTemperatureWarnLowOfM1"  Grid.Column="2" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBlock   Grid.Column="3" Text="预警温度(高):" Margin="0,0,9.5,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxTemperatureWarnHighOfM1" Text="{Binding TempWarnHigh}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" />
                            <TextBox x:Name="tbxNewTemperatureWarnHighOfM1"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />
                            <TextBlock   Grid.Column="6" Text="正常传输间隔:" Margin="0,0,5,0"  HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxNormalIntervalOfM1" Text="{Binding NormalInterval}" Padding="2" Margin="0,0,5,0"  Grid.Column="7" />
                            <TextBox x:Name="tbxNewNormalIntervalOfM1"  Padding="2" Margin="0,0,5,0"  Grid.Column="8" />                            
                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>

                            </Grid.ColumnDefinitions>
                            <TextBlock  Grid.Column="0" Text=" 报警温度(低):" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxTemperatureAlertLowOfM1" Text="{Binding TempAlertLow}"  Grid.Column="1" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBox x:Name="tbxNewTemperatureAlertLowOfM1"  Grid.Column="2" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBlock   Grid.Column="3" Text="报警温度(高):" Margin="0,0,9.5,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxTemperatureAlertHighOfM1" Text="{Binding TempAlertHigh}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" />
                            <TextBox x:Name="tbxNewTemperatureAlertHighOfM1"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />
                            <TextBlock   Grid.Column="6" Text="预警传输间隔:" Margin="0,0,5,0"  HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxWarnIntervalOfM1"  Text="{Binding WarnInterval}" Padding="2" Margin="0,0,5,0"  Grid.Column="7" />
                            <TextBox x:Name="tbxNewWarnIntervalOfM1"  Padding="2" Margin="0,0,5,0"  Grid.Column="8" />
                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>
                            <TextBlock  Grid.Column="0" Text=" 预警湿度(低):" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxHumidityWarnLowOfM1" Text="{Binding HumWarnLow}"  Grid.Column="1" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBox x:Name="tbxNewHumidityWarnLowOfM1"  Grid.Column="2" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBlock   Grid.Column="3" Text="预警湿度(高):" Margin="0,0,9.5,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxHumidityWarnHighOfM1" Text="{Binding HumWarnHigh}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" />
                            <TextBox x:Name="tbxNewHumidityWarnHighOfM1"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />
                            <TextBlock   Grid.Column="6" Text="报警传输间隔:" Margin="0,0,5,0"  HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxAlertIntervalOfM1" Text="{Binding AlertInterval}" Padding="2" Margin="0,0,5,0"  Grid.Column="7" />
                            <TextBox x:Name="tbxNewAlertIntervalOfM1" Padding="2" Margin="0,0,5,0"  Grid.Column="8" />
                        </Grid>

                        <Grid Margin="0 0 0 30">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>

                            </Grid.ColumnDefinitions>
                            <TextBlock  Grid.Column="0" Text=" 报警湿度(低):" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxHumidityAlertLowOfM1" Text="{Binding HumAlertLow}"  Grid.Column="1" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBox x:Name="tbxNewHumidityAlertLowOfM1"  Grid.Column="2" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBlock   Grid.Column="3" Text="报警湿度(高):" Margin="0,0,9.5,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxHumidityAlertHighOfM1" Text="{Binding HumAlertHigh}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" />
                            <TextBox x:Name="tbxNewHumidityAlertHighOfM1"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />
                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="0" Text="IC温度:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox  x:Name="txtICTemperature" Text="{Binding ICTemperature}" Grid.Column="1" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="2" Text="温度:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox  x:Name="txtTemperature" Text="{Binding tempF, StringFormat={}{0:F2}}" Grid.Column="3" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="4" Text="湿度:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="txtHumidity" Text="{Binding humF, StringFormat={}{0:F2}}" Grid.Column="5" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="6" Text="电压:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="txtVolt" Text="{Binding voltF, StringFormat={}{0:F3}}" Grid.Column="7" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="8" Text="RSSI:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox  x:Name="txtRSSI" Text="{Binding RSSI}" Grid.Column="9" Padding="2" Margin="0 0 10 0"/>
                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>
                                <ColumnDefinition/>

                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="0" Text="Flash ID:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="txtFlashID" Text="{Binding FlashID}" Grid.Column="1" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="2" Text="Front:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="txtFlashFront" Text="{Binding FlashFront}" Grid.Column="3" Padding="2" Margin="0 0 10 0"/>

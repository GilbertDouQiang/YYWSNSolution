                            <TextBox x:Name="txtNewFrequency"  Grid.Column="2" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="3" Text="温度补偿:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="txtTemperatureCompensation" Text="{Binding TemperatureCompensation}" Grid.Column="4" Padding="2" Margin="0 0 10 0"/>

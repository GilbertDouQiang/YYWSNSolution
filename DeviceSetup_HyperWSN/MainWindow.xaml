                            <TextBlock   Grid.Column="3" Text="预警温度(高):" Margin="0,0,9.5,0" HorizontalAlignment="Right" />
                            <TextBox  Text="{Binding TemperatureInfoHigh}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" />
                            <TextBox x:Name="txtTemperatureInfoHigh"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />
                            <Button x:Name="btnUpdateApplication" Content="Update App Config" Grid.Column="10" Grid.ColumnSpan="2" Click="btnUpdateApplication_Click" Margin="0.5,0,10,0"/>

                        </Grid>

                        <Grid Margin="0 0 0 10">

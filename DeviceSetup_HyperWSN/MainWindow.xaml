<Window x:Class="DeviceSetup_HyperWSN.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid"
        xmlns:local="clr-namespace:DeviceSetup_HyperWSN"
        mc:Ignorable="d"
        Title="HyperWSN Device Setup" Height="666" Width="1100" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded" Icon="timg_256.ico">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition Height="1.2*"/>
            <RowDefinition Height="10*"/>
        </Grid.RowDefinitions>
        
        <Border Padding="5" Grid.Row="0">
            <StackPanel>
                <Grid Margin="0 8 0 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.4*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="6*"/>
                        <ColumnDefinition Width="1.5*"/>
                        <ColumnDefinition Width="1.5*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="2*"/>
                        <ColumnDefinition Width="0.4*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock Text="端口号:" Grid.Column="1" FontWeight="Bold" Margin="0,3,9,2" VerticalAlignment="Center" HorizontalAlignment="Right" />
                    <ComboBox x:Name="cbSerialPort" Grid.Column="2" Margin="0 0 10 0" VerticalAlignment="Center" Grid.ColumnSpan="1" SelectionChanged="cbSerialPort_SelectionChanged"/>
                    <Button x:Name="btnOpenComport" Grid.Column="3" Content="Open" Click="btnOpenComport_Click"  Margin="0,1,10,1" VerticalAlignment="Center"/>
                    <Button x:Name="btnFindComport" Grid.Column="4" Content="Refresh" Click="btnFindComport_Click" Margin="0.5,0,11,1" VerticalAlignment="Bottom"/>
                </Grid>
                <Grid Margin="0 6 0 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.4*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="0.2*"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="0.2*"/>
                        <ColumnDefinition Width="0.1*"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="0.3*"/>
                        <ColumnDefinition Width="0.2*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="0.4*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="0.2*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="0.4*"/>
                        <ColumnDefinition Width="0.5*"/>
                        <ColumnDefinition Width="0.4*"/>

                    </Grid.ColumnDefinitions>

                    <Button x:Name="btnStartMonitor" Grid.Column="1" Click="btnStartMonitor_Click" Content="Start Monitor" Margin="0,0,0,0" VerticalAlignment="Bottom"/>

                    <TextBox x:Name="tbxStartRxTimeoutThr"   Grid.Column="3" Margin="0,0,0,0" TextAlignment="Right" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Padding="0,0,2,0" MaxLength="2" MaxLines="1" />
                    <TextBlock  Grid.Column="4" Text="秒" Margin="0" HorizontalAlignment="Left" VerticalAlignment="Center" Padding="4,0,0,0"/>

                    <TextBox x:Name="tbxRssiThr"   Grid.Column="6" Margin="0,0,0,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Right" TextAlignment="Right" Padding="0,0,2,0" MaxLength="4" MaxLines="1"   />
                    <TextBlock  Grid.Column="7" Text="dBm" Margin="0,0,0,0" HorizontalAlignment="Left" Padding="4,0,0,0" VerticalAlignment="Center"/>

                    <Button x:Name="btnStopMonitor" Grid.Column="9" Content="Stop Monitor"  Click="btnStopMonitor_Click" VerticalAlignment="Center"/>
                    <Button x:Name="btnCopy" Grid.Column="11" Content="Copy"  Click="btnCopy_Click"  VerticalAlignment="Center"/>
                    <Button x:Name="btnClear" Grid.Column="13" Content="Clear"  Click="btnClear_Click" VerticalAlignment="Center"/>

                    <CheckBox Grid.Column="14" x:Name="cbxLock"  Content="保留" HorizontalAlignment="Left" Margin="4,0,0,0" VerticalAlignment="Center"/>
                    <CheckBox Grid.Column="15" x:Name="cbxBatch"  Content="快捷" HorizontalAlignment="Left" Margin="4,0,0,0" VerticalAlignment="Center"/>

                </Grid>
            </StackPanel>
        </Border>
        
        <Border Grid.Row="1">
            <TabControl Margin="05,0,5,2" >
                <TabItem x:Name="TableOfM1" Header=" M1 | M2 ">
                    <StackPanel x:Name="StackOfM1">

                        <Grid Margin="0 10 0 8">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>

                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" Text="设备名称:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox Grid.Column="1" x:Name="tbxDeviceNameOfM1"  Text="{Binding Name}"  Padding="2" Margin="0 0 10 0"/>

                            <TextBlock Grid.Column="2" Text="设备类型:" Margin="0,0,10,0" HorizontalAlignment="Right" FontWeight="Bold"/>
                            <TextBox Grid.Column="3" x:Name="tbxDeviceTypeOfM1"  Text="{Binding DeviceTypeV,StringFormat='{}{0:X2}'}"  Padding="2" Margin="0 0 10 0"/>

                            <TextBlock Grid.Column="4" Text=" 协议版本号:"  Margin="0,0,10,0" HorizontalAlignment="Right"/>
                            <TextBox Grid.Column="5" x:Name="tbxProtocolOfM1" Text="{Binding ProtocolVersion}"  Padding="2" Margin="0,0,10,0" MaxLength="3" MaxLines="1"/>

                            <Button  Grid.Column="8" x:Name="btnUpdateFactoryOfM1" Content="Update Factory" Click="btnUpdateFactoryOfM1_Click"  Margin="0,0,10,0"/>
                        </Grid>

                        <Grid Margin="0 0 0 8" >
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="1.4*"/>
                                <ColumnDefinition Width="0.6*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="2.6*"/>
                                <ColumnDefinition Width="0.4*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Column="0" Text="PrimaryMac:" Margin="0,0,10,0" HorizontalAlignment="Right" FontWeight="Bold" />
                            <TextBox Grid.Column="1" x:Name="tbxPrimaryMacOfM1" Text="{Binding PrimaryMacS}"   Margin="0 0 10 0" MaxLines="1" MaxLength="11" />

                            <TextBlock Grid.Column="2" Text=" DeviceMac:" Margin="0,0,10,0" HorizontalAlignment="Right" FontWeight="Bold"/>
                            <TextBox Grid.Column="3" x:Name="tbxDeviceMacOfM1"  Text="{Binding DeviceMacS}"  Padding="2" Margin="0 0 0 0" MaxLength="11" MaxLines="1"/>

                            <CheckBox Grid.Column="4" x:Name="cbxIsAllOfM1"  Content="All" HorizontalAlignment="Left" Margin="2,0,0,0" VerticalAlignment="Center"/>

                            <TextBlock  Grid.Column="5" Text=" New Mac:" Margin="0,0,10,0" HorizontalAlignment="Right" FontWeight="Bold"/>
                            <TextBox  Grid.Column="6" x:Name="tbxNewDeviceMacOfM1"  Padding="2" Margin="0 0 10 0" TextChanged="tbxNewDeviceMacOfM1_TextChanged" MaxLength="13" MaxLines="1" AcceptsReturn="True"  />

                            <TextBlock  Grid.Column="7" x:Name="tbkResultOfM1" Text="" Margin="0,0,0,0" HorizontalAlignment="Left" Foreground="Red" VerticalAlignment="Center" FontSize="14"/>
                        </Grid>

                        <Grid Margin="0 0 0 16">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock Grid.Column="0" Text=" 软件版本:"  Margin="0,0,10,0" HorizontalAlignment="Right" FontWeight="Bold"/>
                            <TextBox Grid.Column="1" x:Name="tbxSwRevisionOfM1" Text="{Binding SwRevisionS}" Padding="2" Margin="0,0,10,0" MaxLength="5" MaxLines="1"/>

                            <TextBlock Grid.Column="2" Text="硬件版本:" Margin="0,0,10,0" HorizontalAlignment="Right" FontWeight="Bold" />
                            <TextBox Grid.Column="3" x:Name="tbxHwRevisionOfM1" Text="{Binding HwRevisionS}" Padding="2" Margin="0,0,10,0" MaxLength="11" MaxLines="1" IsReadOnly="True"/>

                            <TextBlock Grid.Column="4" Text="新硬件版本:" Margin="0,0,10,0" HorizontalAlignment="Right" FontWeight="Bold"/>
                            <TextBox Grid.Column="5" x:Name="tbxNewHwRevisionOfM1"   Padding="2" Margin="0,0,10,0" MaxLength="11" MaxLines="1"/>
                        </Grid>

                        <Grid Margin="0 0 0 8">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>
                            <TextBlock  Grid.Column="0" Text=" 客户码:" Margin="0,0,9.5,0" HorizontalAlignment="Right" FontWeight="Bold"/>
                            <TextBox x:Name="tbxCustomerOfM1" Text="{Binding CustomerS}" Grid.Column="1" Padding="2" Margin="0.5,0,10,0" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewCustomerOfM1"   Grid.Column="2" Padding="2" Margin="0.5,0,10,0"/>
                            
                            <TextBlock   Grid.Column="3" Text="Debug:" Margin="0,0,9.5,0" HorizontalAlignment="Right" FontWeight="Bold" />
                            <TextBox x:Name="tbxDebugOfM1" Text="{Binding DebugS}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewDebugOfM1"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />
                            
                            <TextBlock  Grid.Column="6" Text=" 分类码:" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxCategoryOfM1" Text="{Binding Category}"  Grid.Column="7" Padding="2" Margin="0,0,10,0"   />
                            <TextBox x:Name="tbxNewCategoryOfM1"   Grid.Column="8" Padding="2" Margin="0,0,10,0"   />
                            <Button x:Name="btnUpdateUserOfM1" Content="Update User Config" Grid.Column="10" Grid.ColumnSpan="2" Click="btnUpdateUserOfM1_Click"  Margin="0,0,10,0" Height="21" VerticalAlignment="Top"/>
                        </Grid>

                        <Grid Margin="0 0 0 8">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="0" Text="工作模式:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxPatternOfM1" Text="{Binding Pattern}"  Grid.Column="1" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewPatternOfM1"   Grid.Column="2" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="3" Text="传输速率:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxBpsOfM1" Text="{Binding Bps}"  Grid.Column="4" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewBpsOfM1"   Grid.Column="5" Padding="2" Margin="0 0 10 0"/>
                            <TextBlock   Grid.Column="6" Text="发射功率:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxTxPowerOfM1" Text="{Binding TxPower}"  Grid.Column="7" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewTxPowerOfM1"  Grid.Column="8" Padding="2" Margin="0 0 10 0"/>
                        </Grid>

                        <Grid Margin="0 0 0 8">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="0" Text="频段:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxChannelOfM1" Text="{Binding Channel}" Grid.Column="1" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewChannelOfM1"  Grid.Column="2" Padding="2" Margin="0 0 10 0"/>
                            
                            <TextBlock   Grid.Column="3" Text="温度补偿:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxTemperatureCompensationOfM1" Text="{Binding TempCompensation}" Grid.Column="4" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewTemperatureCompensationOfM1"  Grid.Column="5" Padding="2" Margin="0 0 10 0"/>
                            
                            <TextBlock   Grid.Column="6" Text="湿度补偿:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxHumidityCompensationOfM1" Text="{Binding HumCompensation}" Grid.Column="7" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewHumidityCompensationOfM1"  Grid.Column="8" Padding="2" Margin="0,0,10,0"  Height="21" VerticalAlignment="Bottom"/>
                        </Grid>

                        <Grid Margin="0 0 0 8">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="0" Text="存储容量:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxMaxLengthOfM1" Text="{Binding MaxLength}" Grid.Column="1" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="tbxNewMaxLengthOfM1"  Grid.Column="2" Padding="2" Margin="0 0 10 0"/>

                            <TextBlock   Grid.Column="3" Text="温度变化系数:" Margin="0,0,10,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxTempCurveCoeOfM1" Text="{Binding tempCurveCoe, StringFormat='{}{0:F4}'}" Grid.Column="4" Padding="2" Margin="0 0 10 0" MaxLength="6" MaxLines="1" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewTempCurveCoeOfM1"  Grid.Column="5" Padding="2" Margin="0 0 10 0" MaxLength="6" MaxLines="1" />

                            <TextBlock   Grid.Column="6" Text="湿度变化系数:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxHumCurveCoeOfM1" Text="{Binding humCurveCoe, StringFormat='{}{0:F4}'}" Grid.Column="7" Padding="2" Margin="0 0 10 0" MaxLength="6" MaxLines="1" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewHumCurveCoeOfM1"  Grid.Column="8" Padding="2" Margin="0,0,10,0"  Height="21" VerticalAlignment="Bottom" MaxLength="6" MaxLines="1"/>
                        </Grid>

                        <Grid Margin="0 0 0 8">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="0" Text="正常组网间隔:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxAdhocIntervalSucOfM1" Text="{Binding adhocIntervalSuc}" Grid.Column="1" Padding="2" Margin="0 0 10 0" MaxLength="5" MaxLines="1" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewAdhocIntervalSucOfM1"  Grid.Column="2" Padding="2" Margin="0 0 10 0" MaxLength="5" MaxLines="1"/>

                            <TextBlock   Grid.Column="3" Text="异常组网间隔:" Margin="0,0,10,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxAdhocIntervalFaiOfM1" Text="{Binding adhocIntervalFai}" Grid.Column="4" Padding="2" Margin="0 0 10 0" MaxLength="5" MaxLines="1" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewAdhocIntervalFaiOfM1"  Grid.Column="5" Padding="2" Margin="0 0 10 0" MaxLength="5" MaxLines="1" />

                            <TextBlock   Grid.Column="6" Text="ExpHop:" Margin="0,0,10,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxExpHopOfM1" Text="{Binding expHop}" Grid.Column="7" Padding="2" Margin="0 0 10 0" MaxLength="3" MaxLines="1" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewExpHopOfM1"  Grid.Column="8" Padding="2" Margin="0,0,10,0"  Height="21" VerticalAlignment="Bottom" MaxLength="3" MaxLines="1"/>
                        </Grid>

                        <Grid Margin="0 0 0 8">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="0" Text="组网阈值:" Margin="0,0,10,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxAdhocRssiThrOfM1" Text="{Binding adhocRssiThr}" Grid.Column="1" Padding="2" Margin="0 0 10 0" MaxLength="4" MaxLines="1" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewAdhocRssiThrOfM1"  Grid.Column="2" Padding="2" Margin="0 0 10 0" MaxLength="4" MaxLines="1"/>

                            <TextBlock   Grid.Column="3" Text="传输阈值:" Margin="0,0,10,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxTransRssiThrOfM1" Text="{Binding transRssiThr}" Grid.Column="4" Padding="2" Margin="0 0 10 0" MaxLength="4" MaxLines="1" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewTransRssiThrOfM1"  Grid.Column="5" Padding="2" Margin="0 0 10 0" MaxLength="4" MaxLines="1" />

                            <TextBlock   Grid.Column="6" Text="日志模式:" Margin="0,0,10,0" HorizontalAlignment="Right" FontWeight="Bold"/>
                            <TextBox x:Name="tbxLogModeOfM1" Text="{Binding logMode, StringFormat='{}{0:X2}'}" Grid.Column="7" Padding="2" Margin="0 0 10 0" MaxLength="2" MaxLines="1" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewLogModeOfM1"  Grid.Column="8" Padding="2" Margin="0,0,10,0"  Height="21" VerticalAlignment="Bottom" MaxLength="2" MaxLines="1"/>
                        </Grid>

                        <Grid Margin="0 0 0 16">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="3" Text="Catch:" Margin="0,0,10,0" HorizontalAlignment="Right" FontWeight="Bold"/>
                            <TextBox x:Name="tbxCatchOfM1" Text="{Binding catchVal, StringFormat='{}{0:X8}'}" Grid.Column="4" Padding="2" Margin="0 0 10 0" MaxLength="8" MaxLines="1" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewCatchOfM1"  Grid.Column="5" Padding="2" Margin="0 0 10 0" MaxLength="11" MaxLines="1" />

                            <TextBlock   Grid.Column="6" Text="Trap:" Margin="0,0,10,0" HorizontalAlignment="Right" FontWeight="Bold"/>
                            <TextBox x:Name="tbxTrapOfM1" Text="{Binding trapVal, StringFormat='{}{0:X8}'}" Grid.Column="7" Padding="2" Margin="0 0 10 0" MaxLength="8" MaxLines="1" IsReadOnly="True"/>
                            <TextBox x:Name="tbxNewTrapOfM1"  Grid.Column="8" Padding="2" Margin="0,0,10,0"  Height="21" VerticalAlignment="Bottom" MaxLength="11" MaxLines="1"/>
                        </Grid>
                        

                        <Grid Margin="0 0 0 8">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <TextBlock   Grid.Column="3" Text="采/发倍数:" Margin="0,0,9.5,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="tbxSampleSendOfM1" Text="{Binding SampleSend}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" />
                            <TextBox x:Name="tbxNewSampleSendOfM1"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />
                            <TextBlock  Grid.Column="0" Text=" 内部时钟:" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxCalendarOfM1"   Grid.Column="1" Padding="2" Margin="0.5,0,10,0"   Grid.ColumnSpan="2">
                                <TextBox.Text>
                                    <Binding Path="Calendar" StringFormat="{}{0:yyyy-MM-dd HH:mm:ss}"/>
                                </TextBox.Text>
                            </TextBox>
                            <TextBlock  Grid.Column="6" Text=" 采集间隔:" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxIntervalOfM1" Text="{Binding Interval}"  Grid.Column="7" Padding="2" Margin="0,0,5,0"/>
                            <TextBox x:Name="tbxNewIntervalOfM1"  Grid.Column="8" Padding="2" Margin="0,0,5,0"/>
                            <Button x:Name="btnUpdateApplicationOfM1" Content="Update App Config" Grid.Column="10" Grid.ColumnSpan="2" Click="btnUpdateApplicationOfM1_Click" Margin="0,0,10,0"/>
                        </Grid>

                        <Grid Margin="0 0 0 8">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>
                            <TextBlock  Grid.Column="0" Text=" 预警温度(低):" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="tbxTemperatureWarnLowOfM1" Text="{Binding TempWarnLow}"  Grid.Column="1" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBox x:Name="tbxNewTemperatureWarnLowOfM1"  Grid.Column="2" Padding="2" Margin="0.5,0,10,0"/>

                            <TextBox x:Name="txtNewClientID"   Grid.Column="2" Padding="2" Margin="0.5,0,10,0"/>
                            <TextBlock   Grid.Column="3" Text="Debug状态:" Margin="0,0,9.5,0" HorizontalAlignment="Right" />
                            <TextBox x:Name="txtDebug" Text="{Binding DebugString}" Padding="2" Margin="0,0,9.5,0" Grid.Column="4" />
                            <TextBox x:Name="txtNewDebug"  Padding="2" Margin="0,0,9.5,0" Grid.Column="5" />
                            <TextBlock  Grid.Column="6" Text=" 分类码:" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
                            <TextBox x:Name="txtCategory" Text="{Binding Category}"  Grid.Column="7" Padding="2" Margin="0.5,0,10,0"   />
                            <TextBox x:Name="txtNewCategory"   Grid.Column="8" Padding="2" Margin="0.5,0,10,0"   />
                            <ComboBox x:Name="cbUserParameter" Grid.Column="10" Grid.ColumnSpan="2" SelectionChanged="cbUserParameter_SelectionChanged" />



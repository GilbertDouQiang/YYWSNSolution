                            <TextBox x:Name="txtMaxLength" Text="{Binding MaxLength}" Grid.Column="1" Padding="2" Margin="0 0 10 0"/>
                            <TextBox x:Name="txtNewMaxLength"  Grid.Column="2" Padding="2" Margin="0 0 10 0"/>


                        </Grid>

                        <Grid Margin="0 0 0 10">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>

                            </Grid.ColumnDefinitions>
                            <TextBlock  Grid.Column="0" Text=" 采集间隔:" Margin="0,0,9.5,0" HorizontalAlignment="Right"/>
